@model ICollection<Partner>
<div class="row justify-content-center mt-5 pt-4">
    <div class="col-lg-8">
        <div class="text-center mb-4">
            <h4>Our Client's</h4>
            <p class="text-muted">Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium doloremque totam rem aperiam eaque ipsa quae ab illo inventore veritatis et</p>
        </div>
    </div>
</div>

<div class="row align-items-center">
    @foreach (var par in Model)
    {
        <div class="col-md-3">
            <div class="client-images">
                <img src="~/Uploads/images/@par.Photo" alt="@par.Name" class="mx-auto img-fluid d-block">
            </div>
        </div>
    }
</div>
